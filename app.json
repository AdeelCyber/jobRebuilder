{
  "expo": {
    "name": "trainer_app",
    "slug": "trainer_app",
    "version": "1.0.0",
    "scheme": "myapp",
    "host": "myapp.com",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.abdullah.mumtaz65.trainer_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "path": "*",
    "extra": {
      "expoId": "@rayyan/abc123"
    }
  }
}
