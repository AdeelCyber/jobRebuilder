{
  "expo": {
    "name": "trainer_app",
    "slug": "trainer_app",
    "version": "1.0.0",
    "scheme": "myapp",
    "host": "myapp.com",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "buildToolsVersion": "33.0.0",
            "targetSdkVersion": 33
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.abdullah.mumtaz65.trainer_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "path": "*",
    "extra": {
      "expoId": "@rayyan/abc123",
      "eas": {
        "projectId": "c9d5ef72-e8a2-4cb6-9add-6d4fd0a7960c"
      }
    }
  }
}
